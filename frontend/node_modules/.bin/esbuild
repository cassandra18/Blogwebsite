esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/Kemboi/Documents/Github/mentees/Blogwebsite/frontend/node_modules/esbuild/bin/node_modules:/Users/Kemboi/Documents/Github/mentees/Blogwebsite/frontend/node_modules/esbuild/node_modules:/Users/Kemboi/Documents/Github/mentees/Blogwebsite/frontend/node_modules:/Users/Kemboi/Documents/Github/mentees/Blogwebsite/node_modules:/Users/Kemboi/Documents/Github/mentees/node_modules:/Users/Kemboi/Documents/Github/node_modules:/Users/Kemboi/Documents/node_modules:/Users/Kemboi/node_modules:/Users/node_modules:/node_modules:/Users/Kemboi/Documents/Github/mentees/Blogwebsite/frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/Kemboi/Documents/Github/mentees/Blogwebsite/frontend/node_modules/esbuild/bin/node_modules:/Users/Kemboi/Documents/Github/mentees/Blogwebsite/frontend/node_modules/esbuild/node_modules:/Users/Kemboi/Documents/Github/mentees/Blogwebsite/frontend/node_modules:/Users/Kemboi/Documents/Github/mentees/Blogwebsite/node_modules:/Users/Kemboi/Documents/Github/mentees/node_modules:/Users/Kemboi/Documents/Github/node_modules:/Users/Kemboi/Documents/node_modules:/Users/Kemboi/node_modules:/Users/node_modules:/node_modules:/Users/Kemboi/Documents/Github/mentees/Blogwebsite/frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
"$basedir/../esbuild/bin/esbuild"   "$@"
